# Images
image/jpg; feh %s;
image/jpeg; feh %s;
image/png; feh %s;
image/gif; feh %s;
image/bmp; feh %s;

# PDFs
application/pdf; open -a Preview %s; nametemplate=%s.pdf

# HTML
text/html; mutt-open %s; nametemplate=%s.html; test=psg Chrome > /dev/null;
text/html; w3m -dump %s; copiousoutput; nametemplate=%s.html

